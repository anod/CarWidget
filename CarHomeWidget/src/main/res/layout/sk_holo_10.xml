<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/default_background"
    android:gravity="center_vertical"
    android:orientation="vertical"
    tools:ignore="NestedWeights,NestedWeights" >

	<LinearLayout
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:id="@+id/workspace"
			android:layout_width="match_parent"
			android:layout_height="0dip"
			android:layout_weight="1"
			android:baselineAligned="false"
			android:orientation="vertical">

		<include layout="@layout/sk_holo_row" android:id="@+id/row0" />
		<include layout="@layout/sk_holo_row" android:id="@+id/row1" />
		<include layout="@layout/sk_holo_row" android:id="@+id/row2" />
		<include layout="@layout/sk_holo_row" android:id="@+id/row3" />
		<include layout="@layout/sk_holo_row" android:id="@+id/row4" />

	</LinearLayout>

	<include layout="@layout/sk_holo_buttons" />

</LinearLayout>